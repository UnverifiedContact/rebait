YOUTUBE_V3_API_KEY=your_youtube_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.0-flash
WEBSHARE_USERNAME=your_webshare_username
WEBSHARE_PASSWORD=your_webshare_password
TRANSCRIPT_MAX_CONCURRENT_REQUESTS=2

# GROQ API Configuration
# Get your API key from https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here

# Optional: GROQ model to use (default: llama-3.3-70b-versatile)
# All available models (set via GROQ_MODEL env var):
# Llama models:
#   - llama-3.1-8b-instant
#   - llama-3.3-70b-versatile
#   - meta-llama/llama-4-maverick-17b-128e-instruct
#   - meta-llama/llama-4-scout-17b-16e-instruct
# OpenAI models:
#   - openai/gpt-oss-120b
#   - openai/gpt-oss-20b
# Qwen models:
#   - qwen/qwen3-32b
# Moonshot/Kimi models:
#   - moonshotai/kimi-k2-instruct
#   - moonshotai/kimi-k2-instruct-0905
# Groq models:
#   - groq/compound
#   - groq/compound-mini
# Other models:
#   - allam-2-7b
GROQ_MODEL=llama-3.3-70b-versatile

# AI Provider Selection
# Set to "groq" or "gemini" (default: groq)
AI_PROVIDER=groq
