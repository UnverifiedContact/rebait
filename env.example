# Environment Variables Configuration
# Copy this file to .env and update the values

# Transcript Service Configuration
# Host and port for the external transcript service
TRANSCRIPT_SERVICE_HOST=localhost
TRANSCRIPT_SERVICE_PORT=5485

# Gemini AI Configuration
# Your Gemini API key for AI processing
GEMINI_API_KEY=your_gemini_api_key_here
# Gemini model to use (default: gemini-2.0-flash)
GEMINI_MODEL=gemini-2.0-flash

# YouTube Data API v3 Configuration
# Your YouTube Data API v3 key for metadata fetching
YOUTUBE_V3_API_KEY=your_youtube_api_key_here

# Cache Configuration
# Temporary directory for cache storage
TMP=/tmp
